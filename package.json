{
  "name": "nevis-range",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha --reporter spec -t 10000 'test/**/*.js'",
    "postinstall": "npm run build-assets",
    "start": "node index.js",
    "lint": "node_modules/eslint/bin/eslint.js --ext .js,.jsx ./src ./test index.js",
    "build-assets": "node_modules/webpack/bin/webpack.js --optimize-dedupe --optimize-minimize",
    "dev": "mkdir -p public/images/db ; cp public/images/development/*.jpg public/images/db/ ; node_modules/nodemon/bin/nodemon.js -e js,jsx,hbs,json --watch src/ --exec 'node_modules/webpack/bin/webpack.js && npm run start'",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "engines": {
    "node": "7.1.0"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^4.0.5",
    "express": "~4.14.0",
    "express-winston": "^2.3.0",
    "hbs": "~4.0.1",
    "html-to-text": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.2.1",
    "mongodb": "^2.2.11",
    "moniker": "^0.1.2",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "nodemailer": "2.7.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-html-parser": "^1.0.3",
    "react-modal": "^1.6.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.13.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.7.1",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.5.6",
    "nodemon": "^1.11.0"
  }
}
