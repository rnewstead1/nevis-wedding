{
  "name": "nevis-range",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build-assets && node index.js",
    "lint": "node_modules/eslint/bin/eslint.js --ext .js,.jsx ./src ./test index.js",
    "build-assets": "node_modules/webpack/bin/webpack.js",
    "dev": "DEBUG=nevis-range:* node_modules/nodemon/bin/nodemon.js -e js,jsx,hbs,json --watch src/ --exec 'npm run start'",
    "precommit": "npm run lint"
  },
  "engine": {
    "version": "7.1.0"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "es6-promise": "^4.0.5",
    "express": "~4.14.0",
    "hbs": "~4.0.1",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.11",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "^2.7.4",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "husky": "^0.11.9",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.3"
  }
